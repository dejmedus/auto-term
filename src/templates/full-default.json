{
  "setup": [
    {
      "tab": "tab",
      "commands": ["cd folder", "npm install", "clear"],
      "description": "Setup a new tab"
    }
  ],
  "open": [
    {
      "tab": "tab",
      "commands": ["cd folder", "clear"],
      "description": "Open a new tab"
    }
  ],
  "start": [
    {
      "tab": "tab",
      "commands": ["npm start"],
      "description": "Start the tab"
    }
  ],
  "stop": [
    {
      "tab": "tab",
      "commands": ["*STOP", "clear"],
      "description": "Stop the tab"
    }
  ],
  "restart": [
    {
      "tab": "tab",
      "commands": ["*STOP", "clear", "npm start"],
      "description": "Restart the tab"
    }
  ],
  "close": [
    {
      "tab": "tab",
      "commands": ["*CLOSE"],
      "description": "Close the tab"
    }
  ],
  "reminder": [
    {
      "tab": "reminder",
      "commands": ["echo 'you're doing great'"],
      "description": "Reminder"
    }
  ]
}
