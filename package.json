{
  "name": "auto-term-tabs",
  "displayName": "Auto Terminal",
  "description": "Setup and manage terminal tabs",
  "version": "0.0.2",
  "pricing": "Free",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "publisher": "dejmedus",
  "license": "MIT",
  "keywords": [
    "terminal",
    "tabs"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.setupTerminals",
        "title": "Auto Term: setup"
      },
      {
        "command": "extension.openTerminals",
        "title": "Auto Term: Open"
      },
      {
        "command": "extension.startTerminals",
        "title": "Auto Term: Start"
      },
      {
        "command": "extension.stopTerminals",
        "title": "Auto Term: Stop"
      },
      {
        "command": "extension.restartTerminals",
        "title": "Auto Term: Restart"
      },
      {
        "command": "extension.closeTerminals",
        "title": "Auto Term: Close"
      },
      {
        "command": "extension.customActions",
        "title": "Auto Term: Action"
      },
      {
        "command": "extension.showUsageGuide",
        "title": "Auto Term: Show Usage Guide"
      },
      {
        "command": "extension.getTemplate",
        "title": "Auto Term: Template"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "postcompile": "copyfiles -u 2 src/templates/* out/templates",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}
